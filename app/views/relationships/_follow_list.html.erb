<% if @users.exists? %>
  <% @users.each do |user| %>
    <table class="table">
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th></th>
          <th></th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td><%= user.profile_image %></td>
          <td><%= user.name %></td>
          <td>フォロー数: <%= user.followings.count %></td>
          <td>フォロワー数: <%= user.followers.count %></td>
          <td><%= link_to "show",user_path(current_user) %></td>
        </tr>
      </tbody>
    </table>
  <% end %>
<% else %>
  <tbody>
  <tr>
   <td>ユーザーはいません</td>
  </tr>
  </tbody>

<% end %>